{
  "name": "kraimer",
  "version": "0.0.1",
  "description": "Kraimer AI Pipeline",
  "main": "dist/index.js",
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "build": "rm -rf dist && npx tsc",
    "db:migrate:make": "knex migrate:make --esm --client postgresql",
