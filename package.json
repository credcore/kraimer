  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://https://github.com/credcore/kraimer-next.git"
  },
  "dependencies": {
    "@types/knex": "^0.16.1",
    "@types/node": "^20.14.2",
    "@types/pg-promise": "^5.4.3",
    "@types/yargs": "^17.0.32",
    "knex": "^3.1.0",
    "pg-promise": "^11.8.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "typescript": "^5.4.5"
  }
}
